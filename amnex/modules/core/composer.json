{
    "name": "wezom/core",
    "autoload": {
        "psr-4": {
            "Wezom\\Core\\": "src/",
            "Wezom\\Core\\Database\\Factories\\": "database/factories/",
            "Wezom\\Core\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wezom\\Core\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "bensampo/laravel-enum": "^6.0",
        "ckfinder/ckfinder-laravel-package": "^5.0",
        "laravel/framework": "^11.9",
        "laravel/helpers": "*",
        "laravel/horizon": "^v5.29",
        "laravel/octane": "^2.5",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.9",
        "mll-lab/laravel-graphiql": "^3.2",
        "nuwave/lighthouse": "^6.45",
        "spatie/laravel-data": "^4.6",
        "spatie/laravel-medialibrary": "^11.0",
        "spatie/laravel-permission": "^6.2",
        "tucker-eric/eloquentfilter": "^3.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^2.9",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^11.0.1",
        "spatie/laravel-ignition": "^2.0",
        "squizlabs/php_codesniffer": "^3.8"
    },
    "suggest": {
        "ckfinder/ckfinder-laravel-package": "Required for admin file manager (^5.0)",
        "wezom/admins": "Required for back-office related schema stuff"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wezom\\Core\\CoreServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Boreyko Vladyslav"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "library",
    "version": "1.0.0"
}
