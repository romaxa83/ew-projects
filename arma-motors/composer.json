{
    "name": "wezom/arma-motors",
    "type": "project",
    "description": "Backend for arma-motors app",
    "license": "MIT",
    "authors": [
        {
            "name": "Rodomanov Roman",
            "email": "rodomanov.r.wezom@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "barryvdh/laravel-dompdf": "^1.0",
        "darkaonline/l5-swagger": "^8.0",
        "fideloper/proxy": "^4.4",
        "firebase/php-jwt": "^5.2",
        "fruitcake/laravel-cors": "^2.0",
        "grimzy/laravel-mysql-spatial": "^4.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.12",
        "laravel/passport": "^10.1",
        "laravel/telescope": "^4.6",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "mll-lab/graphql-php-scalars": "^3.1",
        "mll-lab/laravel-graphql-playground": "^2.5",
        "nuwave/lighthouse": "^4.16",
        "phpoffice/phpspreadsheet": "1.16",
        "ramsey/uuid": "^4.1",
        "rennokki/laravel-eloquent-query-cache": "^2.6",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-permission": "^4.0",
        "telegram-bot/api": "^2.3",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.9",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "haydenpierce/class-finder": "^0.4.3",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "ide": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -N",
            "@php artisan lighthouse:ide-helper"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
