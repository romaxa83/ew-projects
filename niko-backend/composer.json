{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "daaner/turbosms": "^1.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "grimzy/laravel-mysql-spatial": "^3.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "laravel/framework": "^7.29",
        "laravel/passport": "8.4",
        "laravel/tinker": "^2.5",
        "lcobucci/jwt": "3.3.3",
        "mjaschen/phpgeo": "^3.2",
        "nyholm/psr7": "^1.3",
        "phpoffice/phpspreadsheet": "^1.15",
        "wezom-cms/articles": "^7.0",
        "wezom-cms/cars": "^7.0",
        "wezom-cms/core": "^7.0",
        "wezom-cms/dealerships": "^7.0",
        "wezom-cms/firebase": "^7.0",
        "wezom-cms/promotions": "^7.0",
        "wezom-cms/regions": "^7.0",
        "wezom-cms/requests": "^7.0",
        "wezom-cms/services": "^7.0",
        "wezom-cms/services-orders": "^7.0",
        "wezom-cms/supports": "^7.0",
        "wezom-cms/telegram-bot": "^7.0",
        "wezom-cms/translates": "^7.0",
        "wezom-cms/users": "^7.0",
        "yadakhov/insert-on-duplicate-key": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.8",
        "facade/ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.3",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./modules/*"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
